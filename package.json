{
  "name": "nativescript-plugin-universal-links",
  "version": "1.0.1",
  "description": "Universal links (IOS) and App Links (Android) support for NativeScript.",
  "main": "build/plugin-universal-links",
  "typings": "build/plugin-universal-links.common.d.ts",
  "scripts": {
    "build": "npm run setup && npx rimraf build/ && npx tsc -p tsconfig.json && npx tns plugin build",
    "prepack": "npm run build",
    "setup": "npm i && ts-patch install"
  },
  "keywords": [
    "nativescript",
    "plugin",
    "ios",
    "android",
    "universal",
    "link",
    "deeplink"
  ],
  "author": "Dragos Sebestin",
  "license": "MIT",
  "repository": "https://github.com/sebestindragos/nativescript-plugin-universal-links",
  "devDependencies": {
    "@nativescript/core": "7.1.0",
    "@nativescript/types": "7.1.0",
    "@nativescript/webpack": "4.0.0",
    "nativescript": "7.1.1",
    "rimraf": "3.0.2",
    "ts-patch": "1.3.1",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "url-parse": "1.4.7"
  }
}
